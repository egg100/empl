<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="300" prefWidth="600" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1"
	fx:controller="myapp.exercise.RootController">
	<center>
		<TableView fx:id="tableView" prefHeight="193.0" prefWidth="600.0">
			<columns>
				<TableColumn prefWidth="150" text="이름" />
				<TableColumn prefWidth="150" text="국어" />
				<TableColumn prefWidth="150" text="수학" />
				<TableColumn prefWidth="150" text="영어" />
			</columns>
		</TableView>
	</center>
	<bottom>
		<HBox prefHeight="70.0">
			<Button text="추가" onAction="#handleBtnAddAction">
            <HBox.margin>
               <Insets left="250.0" top="20.0" />
            </HBox.margin></Button>
		</HBox>
	</bottom>
</BorderPane>
